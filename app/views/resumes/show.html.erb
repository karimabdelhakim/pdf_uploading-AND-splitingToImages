<p id="notice"><%= notice %></p>


<p>
  <strong>Name:</strong>
  <%= @resume.name %>
</p>

<p>
  <strong>Content:</strong>
  <%= @resume.attachment %>
</p>

<p>
  <strong>Slides:</strong>
</p>

<%= link_to 'image' ,  "/uploads/resume/attachment/#{@resume.id}/#{@resume.attachment_identifier}1.png" %>


<%= link_to 'Back', resumes_path %>
<% images_path = Array.new  %>


<%  x = "/uploads/resume/attachment/#{@resume.id}/#{@resume.attachment_identifier}1.png"       %>


<% images_names = Dir.entries("public/uploads/resume/attachment/#{@resume.id}").select {|f| !File.directory? f} %>
<%  images_names.shift %>
<% images_names.sort! %>


<% images_names.each do |imageName|   %>
<% images_path.push("/uploads/resume/attachment/#{@resume.id}/#{imageName}" ) %>
<% end %>

<img src= "<%= images_path[0] %>" alt="rss feed" />




